
app.factory("services", ['$http', function($http) {
        var serviceBase = 'http://localhost:3000/produto/';
        var obj = {};

       //where u will code the api service
       
     obj.getProductList = function(pname){
            return $http.get(serviceBase + pname);
    };
    
    
    
        return obj;
    }
    ]);
